.columnPage{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  width:100%;
  height: 100%;
}
.shangban{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  height: 50%;
}
.sanfenzhiyi{
  display: flex;
  flex-direction: row;
  height: 33.33%;
  width: 100%;
  justify-content: space-between;
  align-items: center;
  border: 1rpx solid #F0F0F0;
}
.youban{
  width: 10rpx;
  height: 20rpx;
  margin-right: 30rpx;
}
.zuoban{

  margin-left: 40rpx;
  display: flex;
  height: 46%;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  
}
.xiaban{
  background: #F0F0F0;
  width: 100%;
  height: 50%;
  display: flex;
  flex-direction: column;
  justify-content: start;
  align-items: center;
}
.xiaoziti{
  font-size: 28rpx;

}
.daziti{
  font-size: 30rpx;

}
.item{
  margin-top: 20%;
  background-color: #28A9FA;
  width: 85%;
  height: 14%;
  display: flex;
  border-radius: 40rpx;
  flex-direction: row;
  justify-content: center;
  align-items: center;

}
.sousuo{
  font-size: 35rpx;
  color: white;
  
}